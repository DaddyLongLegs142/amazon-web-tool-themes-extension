{
  "manifest_version": 2,
  "name": "Amazon Web Tool Themes",
  "version": "0.1",

  "description": "A library of themes for various Amazon FC web tools",

  "icons": {
    "48": "icons/FC-Logo"
  },

  "permissions": ["activeTab"],

  "browser_action": {
    "default_icon": "icons/FC-Logo",
    "default_title": "Change Theme",
    "default_popup": "popup/choose_theme.html"
  },

  "web_accessible_resources": [
    "css/*"
  ]

}
